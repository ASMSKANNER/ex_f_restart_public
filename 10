docker rm -f exorde-cli5
docker rm -f exorde-cli6
docker rm -f exorde-cli7
docker rm -f exorde-cli8
docker rm -f exorde-cli9
docker rm -f exorde-cli10
docker run -d --restart unless-stopped --pull always --name exorde-cli rg.fr-par.scw.cloud/exorde-labs/exorde-cli -m $fuckingMMexorde -l 4
